{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/core": "7.12.10",
    "@svgr/webpack": "5.5.0",
    "@types/jest": "^26.0.0",
    "@types/node": "^14.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.0.0",
    "@typescript-eslint/parser": "^4.0.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "^26.0.0",
    "babel-loader": "8.2.2",
    "babel-plugin-named-asset-import": "^0.3.6",
    "babel-preset-react-app": "^9.1.1",
    "camelcase": "^6.0.0",
    "case-sensitive-paths-webpack-plugin": "2.3.0",
    "classnames": "^2.2.6",
    "css-loader": "^5.0.0",
    "dotenv": "8.2.0",
    "dotenv-expand": "5.1.0",
    "eslint": "^7.0.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-loader": "4.0.2",
    "eslint-plugin-flowtype": "5.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "file-loader": "6.2.0",
    "fs-extra": "^9.0.0",
    "html-webpack-plugin": "4.5.1",
    "identity-obj-proxy": "3.0.0",
    "jest": "26.6.3",
    "jest-environment-jsdom-fourteen": "1.0.1",
    "jest-resolve": "26.6.2",
    "jest-watch-typeahead": "0.6.1",
    "mini-css-extract-plugin": "1.3.3",
    "optimize-css-assets-webpack-plugin": "5.0.4",
    "pnp-webpack-plugin": "1.6.4",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-loader": "4.1.0",
    "postcss-normalize": "9.0.0",
    "postcss-preset-env": "6.7.0",
    "postcss-safe-parser": "5.0.2",
    "react": "^17.0.0",
    "react-app-polyfill": "^2.0.0",
    "react-dev-utils": "^11.0.0",
    "react-dom": "^17.0.0",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "resolve": "1.19.0",
    "resolve-url-loader": "3.1.2",
    "sass-loader": "10.1.0",
    "semver": "7.3.4",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "5.1.0",
    "ts-pnp": "1.2.0",
    "typescript": "~4.1.0",
    "url-loader": "4.1.1",
    "webpack": "5.12.2",
    "webpack-dev-server": "3.11.1",
    "webpack-manifest-plugin": "3.0.0",
    "workbox-webpack-plugin": "6.0.2"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js",
    "test:styles": "yarpm storybook:build && (node scripts/test-styles.js || open .loki/report.html)",
    "test:styles:update": "yarn loki approve",
    "test:e2e": "PORT=8080 BROWSER=none start-server-and-test start http://localhost:8080 e2e:run",
    "test:e2e:open": "PORT=8080 BROWSER=none start-server-and-test start http://localhost:8080 e2e:open",
    "e2e:run": "cypress run",
    "e2e:open": "cypress open",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "storybook:build": "build-storybook -c .storybook -s .storybook/static-link -o .storybook/static-link",
    "coverage": "jest --coverage && cat ./coverage/lcov.info | coveralls"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:cypress/recommended"
    ],
    "plugins": [
      "cypress"
    ],
    "env": {
      "cypress/globals": true
    },
    "rules": {
      "import/no-extraneous-dependencies": [
        "error",
        {
          "devDependencies": [
            "cypress/**/*"
          ]
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [
      "<rootDir>/src/"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.8.3",
    "@storybook/addon-docs": "^6.0.0",
    "@storybook/addon-knobs": "^6.0.0",
    "@storybook/addon-viewport": "^6.0.0",
    "@storybook/react": "^6.0.0",
    "@types/classnames": "^2.2.9",
    "@types/enzyme": "^3.10.5",
    "@types/react-redux": "^7.1.7",
    "@types/redux-mock-store": "^1.0.2",
    "coveralls": "^3.0.9",
    "cypress": "^6.0.0",
    "cypress-cucumber-preprocessor": "^4.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint-plugin-cypress": "^2.10.3",
    "loki": "^0.27.0",
    "redux-mock-store": "^1.5.4",
    "reg-cli": "^0.16.0",
    "start-server-and-test": "^1.10.10",
    "yarpm": "^0.2.1"
  },
  "loki": {
    "configurations": {
      "chrome.small": {
        "target": "chrome.docker",
        "width": 320,
        "height": 240,
        "deviceScaleFactor": 1,
        "mobile": false
      },
      "chrome.medium": {
        "target": "chrome.docker",
        "width": 800,
        "height": 600,
        "deviceScaleFactor": 1,
        "mobile": false
      },
      "chrome.large": {
        "target": "chrome.docker",
        "width": 1280,
        "height": 1024,
        "deviceScaleFactor": 1,
        "mobile": false
      }
    },
    "verboseRenderer": true,
    "chromeTolerance": 0.01
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true
  }
}
